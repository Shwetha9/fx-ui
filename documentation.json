            "description": "",
            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { CommonModule } from '@angular/common';\nimport { Component, OnInit, input, computed, output } from '@angular/core';\nimport { FxUiButtonType, FxUiButtonSize, FxUiButtonColor, FxUiButtonShape, FxUiButtonIconPosition, FxUiButtonState } from './fx-ui-button.component.type';\n\n@Component({\n  selector: 'fx-ui-button',\n  standalone: true,\n  imports: [\n    CommonModule,\n  ],\n  templateUrl: './fx-ui-button.component.html',\n  styleUrls: ['./fx-ui-button.component.scss'],\n})\nexport class FxUiButtonComponent implements OnInit {\n\n  type = input<FxUiButtonType>('submit');\n  size = input<FxUiButtonSize>('medium');\n  color = input<FxUiButtonColor>('secondary');\n  shape = input.required<FxUiButtonShape>();\n  iconPosition = input.required<FxUiButtonIconPosition>();\n  state = input.required<FxUiButtonState>();\n  label = input.required<string>();\n  buttonClickEvent = output<void>();\n  classModifiers: {[key:string]:boolean} = {};\n  icon = input<string>();\n\n  ngOnInit(): void {\n    this.classModifiers = this.generateButtonClass();\n  }\n\n  generateButtonClass(): {[key:string]:boolean}  {\n    return {\n      [`fx-ui-button--${this.size()}`]: !!this.size() ,\n      [`fx-ui-button--${this.color()}`]: !!this.color() ,\n      [`fx-ui-button--${this.shape()}`]: !!this.shape(),\n      [`fx-ui-button--${this.iconPosition()}`]: !!this.iconPosition(),\n      [`fx-ui-button--${this.state()}`]: !!this.state(),\n    };\n\n  };\n\n  handleButtonClick(): void {\n    this.buttonClickEvent.emit();\n  }\n\n  // Computed properties\n  readonly isDisabled = computed(() => this.state() === 'disabled' || this.state() === 'loading');\n}\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "/* Basic button styles */\n\n@use '../../../../assets/scss/variables' as *;\n\n.fx-ui-button {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  border: none;\n  cursor: pointer;\n  transition: all 0.3s ease;\n  border-radius: 4px;\n\n  &-submit {\n  }\n  &-navigate {\n  }\n  &--primary {\n    background-color: $primary;\n    color: #fff;\n    &:hover {\n      background-color: light()\n    }\n  }\n  &--small {\n    padding: 0.25rem 0.5rem;\n    font-size: 1rem;\n  }\n  &--medium {\n    padding: 1.25rem 2.5rem;\n    font-size: 1.5rem;\n  }\n  &--large {\n    padding: 0.75rem 1.5rem;\n    font-size: 1.75rem;\n  }\n}\n",
                    "styleUrl": "./fx-ui-button.component.scss"
                }
            ],
            "stylesData": "",
            "extends": [],
            "implements": [
                "OnInit"
            ],
            "templateData": "<button\n  [attr.aria-label]=\"label()\"\n  [attr.aria-disabled]=\"isDisabled()\"\n  [attr.aria-pressed]=\"state() === 'active'\"\n  [attr.aria-describedby]=\"label() + '-description'\"\n  class=\"fx-ui-button\"\n  (click)=\"handleButtonClick()\"\n  [ngClass]=\"classModifiers\"\n  [disabled]=\"isDisabled()\"\n>\n  <ng-container *ngIf=\"iconPosition() === 'left'\">\n    <span class=\"fx-button-icon--left\">\n      {{ icon() }}\n    </span>\n  </ng-container>\n  {{ label() }}\n  <ng-container *ngIf=\"iconPosition() === 'right'\">\n    <span class=\"fx-button-icon--right\">\n      {{ icon() }}\n    </span>\n  </ng-container>\n</button>\n"
        },
